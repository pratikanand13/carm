{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "env": {
      "MONGO_URI": "@mongo_uri",
      "JWT_SECRET": "@jwt_secret"
    },
    "functions": {
      "api/*.js": {
        "maxDuration": 10,
        "memory": 512
      }
    },
    "static": {
      "uploads": "uploads"
    }
  }
  